package com.example.firstTask.model.VO;




import jakarta.annotation.Nonnull;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.SequenceGenerator;


@Entity
@SequenceGenerator(initialValue=202201, name="custseq")
public class CustomerVO {
	
	@Id
	@GeneratedValue(strategy=GenerationType.SEQUENCE, generator="custseq")
	private int customerId;
	
	@Nonnull
	private String customerName;
	
//	@Nonnull
//	@Email(regexp = ".+[@].+[\\.].+")
	private String emailId;
	
//	@Length(min=10,max=10)
	private String mobNo;
	
	
	public CustomerVO() {
		super();
	}


	public CustomerVO(int customerId, String customerName, String emailId, String mobNo) {
		super();
		this.customerId = customerId;
		this.customerName = customerName;
		this.emailId = emailId;
		this.mobNo = mobNo;
	}


	public int getCustomerId() {
		return customerId;
	}


	public void setCustomerId(int customerId) {
		this.customerId = customerId;
	}


	public String getCustomerName() {
		return customerName;
	}


	public void setCustomerName(String customerName) {
		this.customerName = customerName;
	}


	public String getEmailId() {
		return emailId;
	}


	public void setEmailId(String emailId) {
		this.emailId = emailId;
	}


	public String getMobNo() {
		return mobNo;
	}


	public void setMobNo(String mobNo) {
		this.mobNo = mobNo;
	}


	@Override
	public String toString() {
		return "Customer [customerId=" + customerId + ", customerName=" + customerName + ", emailId=" + emailId
				+ ", mobNo=" + mobNo + "]";
	}
}
